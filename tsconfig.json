{
  "compilerOptions": {
    "target": "es2022",
    "moduleDetection": "force",
    "isolatedModules": true,
    "verbatimModuleSyntax": true,
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "noImplicitOverride": true,
    "module": "NodeNext",
    "outDir": "dist",
    "lib": ["es2022"],
    "types": ["node"]
  }
}
